{
    "name": "discord-rich-presence-integration",
    "displayName": "Discord Rich Presence Integration",
    "description": "Allows other people on discord to see what you're currently working on (Applied after restart)",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
        "configuration": {
            "title": "Discord Rich Presence",
            "properties": {
                "discord-rich-presence-integration.startWithVSCode": {
                    "description": "Show Rich Presence on Startup",
                    "type": "boolean",
                    "default": true,
                    "scope": "application"
                }
            }
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.50.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.27",
        "eslint": "^7.6.0",
        "glob": "^7.1.6",
        "mocha": "^8.0.1",
        "typescript": "^3.8.3",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "discord-rich-presence": "0.0.8"
    }
}